<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>true</IsPackable>
    <PublishAot Condition="'$(BeamOsEnableAot)' == 'true'">true</PublishAot>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
    <CompilerGeneratedFilesOutputPath>$(BaseIntermediateOutputPath)/Generated</CompilerGeneratedFilesOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Riok.Mapperly" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" />
    <!-- DotWrap reference add in directory build props -->
    <!-- <PackageReference Include="DotWrap" /> -->
    <PackageReference Include="Microsoft.Extensions.Http" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BeamOs.StructuralAnalysis.ApiClient\BeamOs.StructuralAnalysis.ApiClient.csproj" />
  </ItemGroup>

</Project>
