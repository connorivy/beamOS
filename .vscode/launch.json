{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch StructuralAnalysis.Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/StructuralAnalysis/BeamOs.StructuralAnalysis.Api/bin/Debug/net9.0/BeamOs.StructuralAnalysis.Api.dll",
            "cwd": "${workspaceFolder}/src/StructuralAnalysis/BeamOs.StructuralAnalysis.Api",
            "env": {},
            "justMyCode": true,
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "http://localhost:5000",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start http://localhost:5000"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "requireExactSource": false
        },
        {
            "name": "Launch WebApp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/WebApp/BeamOs.WebApp/bin/Debug/net9.0/BeamOs.WebApp.dll",
            "cwd": "${workspaceFolder}/src/WebApp/BeamOs.WebApp",
            "env": {},
            "justMyCode": true,
            "console": "internalConsole",
            "stopAtEntry": false,
            "requireExactSource": false
        }
    ],
    "compounds": [
        {
            "name": "FullStack",
            "configurations": [
                "Launch StructuralAnalysis.Api",
                "Launch WebApp"
            ]
        }
    ]
}