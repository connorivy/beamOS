<Project>
  <Target Name="asdf" BeforeTargets="Build">
    <Exec Command="echo DEBUG PRINT: buildPath: $(MSBuildProjectBuildPath)" />
    <!-- <Exec Command="echo DEBUG PRINT: publishPath: $(MSBuildProjectPublishPath)" /> -->
    <Exec Command="echo DEBUG PRINT: isPublishing: $(_IsPublishing)" />
    <Exec Command="echo DEBUG PRINT: dotWrapToolsPath: $(DotWrapToolsPath)" />
    <Exec Command="echo DEBUG PRINT: DotWrapGenerateAfterBuild: $(DotWrapGenerateAfterBuild)" />
    <Exec Command="echo DEBUG PRINT: WrapperGenTarget: $(WrapperGenTarget)" />
    <Exec Command="echo DEBUG PRINT: WrapperGenTarget: $(WrapperGenTarget)" />
  </Target>
  <PropertyGroup Condition="'$(DotWrapRelativeLocation)' != ''">
    <Configuration Condition=" '$(Configuration)' == '' and '$(_IsPublishing)' == 'true' "
      >Release</Configuration
    >
    <MSBuildProjectBuildPath>$(DotWrapRelativeLocation)/src/DotWrap.MSBuild/bin/$(Configuration)/$(TargetFramework)</MSBuildProjectBuildPath>
    <MSBuildProjectPublishPath>$(DotWrapRelativeLocation)/src/DotWrap.MSBuild/bin/$(Configuration)/$(TargetFramework)/$(RuntimeIdentifier)</MSBuildProjectPublishPath>
    <DotWrapToolsPath Condition="'$(_IsPublishing)' != 'true'"
      >$(MSBuildProjectBuildPath)</DotWrapToolsPath
    >
    <DotWrapToolsPath Condition="'$(_IsPublishing)' == 'true'"
      >$(MSBuildProjectPublishPath)</DotWrapToolsPath
    >
  </PropertyGroup>
</Project>
